A,CAAA,CAAA;AACA,WAAA,OAAA,CAAA,MAAA,GAAA,KAAA,aAAA;AACA,KAzCA;AA0CA,IAAA,QA1CA,sBA0CA;AAAA;;AACA,UAAA,QAAA,GAAA,IAAA,QAAA,EAAA;AACA,MAAA,QAAA,CAAA,MAAA,CAAA,QAAA,EAAA,KAAA,aAAA;AACA,MAAA,QAAA,CAAA,MAAA,CAAA,IAAA,EAAA,YAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,MAAA,KAAA,GACA,GADA,CACA,gBADA,EAEA,QAFA,EAGA;AACA,QAAA,OAAA,EAAA,KAAA;AADA,OAHA,EAMA,IANA,CAMA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,aAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,KAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,QAAA,CAAA,IAAA,CAAA,MAAA,EACA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA,CAFA;AAGA,OAZA,EAYA,KAZA,CAYA,UAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,OAdA;AAeA,KA9DA;AA+DA,IAAA,UA/DA,wBA+DA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA;AAjEA,GAnCA;AAuGA,EAAA,QAAA,oBACA,QAAA,CAAA,CAAA,WAAA,CAAA,CADA;AAvGA,CAAA","sourcesContent":["<template>\n    <v-container fluid fill-height class='bg'>\n        <v-row class='mt-3 ml-3'>\n            <p class=\"text-h6\"> プロフィール編集</p>\n        </v-row>\n        <v-row class='ml-1'>\n            <v-btn icon @click='returnPage'>\n                <v-icon>\n                    mdi-arrow-left-thick\n                </v-icon>\n            </v-btn>\n        </v-row>\n        <v-layout wrap class='justify-center'>\n            <v-flex xs12 sm9 md6>\n                <v-divider class='mt-5'/>\n                <v-row v-if='avatar' justify=\"center\" class='mt-10 mb-15'>\n                    <v-btn\n                        icon\n                        @click=\"dialog_avatar = true\"\n                    >\n                        <v-avatar size='100'>\n                            <v-img\n                            v-bind:src=\"railsURL + avatar\"\n                            >\n                            </v-img>\n                        </v-avatar>\n                    </v-btn>\n                    <v-dialog\n                        :retain-focus=\"false\"\n                        v-model=\"dialog_avatar\"\n                        persistent\n                        max-width=\"600px\"\n                        >\n                        <v-card>\n                            <v-card-title>\n                                プロフィール画像を変更\n                            </v-card-title>\n                            <input name=\"uploadedImage\" type=\"file\" ref=\"file\" accept=\"image/png, image/jpeg\" @change=\"onFileChange\"><br />\n\n                            <v-card-actions>\n                            <v-spacer></v-spacer>\n                            <v-btn icon @click='postItem'>\n                                <v-icon>\n                                    mdi-book\n                                </v-icon>\n                            </v-btn>\n                            <v-btn\n                                @click=\"dialog_avatar = false\"\n                                icon\n                            >\n                                <v-icon>\n                                    mdi-close\n                                </v-icon>\n                            </v-btn>\n                            </v-card-actions>\n                        </v-card>\n                    </v-dialog>\n                </v-row>\n                <v-row v-else justify=\"center\" class='mt-10 mb-15'>\n                    <v-btn\n                        icon\n                        @click=\"dialog_avatar = true\"\n                    >\n                        <v-avatar size='100'>\n                            <v-img\n                            src=\"@/assets/icon/kame.jpg\"\n                            >\n                            </v-img>\n                        </v-avatar>\n                    </v-btn>\n                    <v-dialog\n                        :retain-focus=\"false\"\n                        v-model=\"dialog_avatar\"\n                        persistent\n                        max-width=\"600px\"\n                        >\n                        <v-card>\n                            <v-card-title>\n                                プロフィール画像を変更\n                            </v-card-title>\n                            <input name=\"uploadedImage\" type=\"file\" ref=\"file\" accept=\"image/png, image/jpeg\" @change=\"onFileChange\"><br />\n\n                            <v-card-actions>\n                            <v-spacer></v-spacer>\n                            <v-btn icon @click='postItem'>\n                                <v-icon>\n                                    mdi-book\n                                </v-icon>\n                            </v-btn>\n                            <v-btn\n                                @click=\"dialog_avatar = false\"\n                                icon\n                            >\n                                <v-icon>\n                                    mdi-close\n                                </v-icon>\n                            </v-btn>\n                            </v-card-actions>\n                        </v-card>\n                    </v-dialog>\n                </v-row>\n\n            <v-card\n                height = \"450\"\n                rounded=\"xl\"\n            >\n                <v-container>\n                    <v-row>\n                        <v-col>\n                            <v-list two-line>\n                                <template v-for=\"(item, key, index) in items\">\n                                    <v-list-item :key = key>\n                                        <v-list-item-content>\n                                            <v-list-item-title>\n                                                {{key}}\n                                            </v-list-item-title>\n                                            <v-row>\n                                                <v-col cols=\"9\">\n                                                    <v-list-item-subtitle>\n                                                        {{item}}\n                                                    </v-list-item-subtitle>\n                                                </v-col>\n                                                <v-col>  \n                                                    <div v-if='index === 0'>                                              \n                                                        <v-btn icon @click='dialog_0 = true'>\n                                                            <v-icon>\n                                                                mdi-pen\n                                                            </v-icon>\n                                                        </v-btn>\n                                                    </div>\n\n                                                    <div v-if='index === 1'>                                              \n                                                        <v-btn icon @click='dialog_1 = true'>\n                                                            <v-icon>\n                                                                mdi-pen\n                                                            </v-icon>\n                                                        </v-btn>\n                                                    </div>\n                                                </v-col>\n                                            </v-row>\n                                        </v-list-item-content>\n                                    </v-list-item>\n                                    <v-divider\n                                        v-if=\"index < Object.keys(items).length\"\n                                        :key=\"item\"\n                                    ></v-divider>\n                                </template>\n                            </v-list>\n                        </v-col>\n                    </v-row>\n                </v-container>\n            </v-card>\n\n            <v-dialog\n                v-model=\"dialog_1\"\n                persistent\n                max-width=\"500\"\n            >\n                <v-card rounded=\"xl\">\n                    <v-card-title>\n                    <span class=\"headline\">生年月日</span>\n                    </v-card-title>\n                    <v-card-text>\n                    <v-container>\n                            <v-row>\n                        <v-col>\n                            <v-select\n                            v-model=\"year\"\n                            :items=\"yearrange\"\n                            label=\"年\"\n                            ></v-select>\n                        </v-col>\n                        \n                        <v-col>\n                            <v-select\n                            v-model=\"month\"\n                            :items=\"monthrange\"\n                            label=\"月\"\n                            ></v-select>\n                        </v-col>\n                        <v-col>\n                            <v-select\n                            v-model=\"day\"\n                            :items=\"daysrange\"\n                            label=\"日\"\n                            ></v-select>\n                        </v-col>\n                        </v-row>\n                    </v-container>\n                \n                    </v-card-text>\n                    <v-card-actions>\n                    <v-spacer></v-spacer>\n                    <v-btn\n                        color=\"blue darken-1\"\n                        text\n                        @click=\"dialog_1 = false\"\n                    >\n                        Close\n                    </v-btn>\n                    <v-btn\n                        icon\n                        dialog_1 = \"false\"\n                        @click='registerBirthdate'\n                    >\n                        <v-icon> mdi-check</v-icon>\n                    </v-btn>\n                    </v-card-actions>\n                </v-card>\n            </v-dialog>\n\n            <v-dialog\n                v-model=\"dialog_0\"\n                persistent\n                max-width=\"500\"\n            >\n            <v-card rounded=\"xl\"> \n                <v-row class=\"ml-3 mr-3 mt-7\">\n                <v-col>\n                    <v-text-field\n                        v-model=\"lastname\"\n                        label=\"姓\"\n                        outlined\n                        filled\n                        rounded\n                        background-color=\"grey lighten-5\"\n                    ></v-text-field>\n                </v-col>\n                <v-col>\n                    <v-text-field\n                        v-model=\"firstname\"\n                        label=\"名\"\n                        outlined\n                        filled\n                        rounded\n                        background-color=\"grey lighten-5\"\n                    ></v-text-field>\n                </v-col>\n            </v-row>\n                <v-card-actions>\n                <v-spacer></v-spacer>\n                <v-btn\n                    color=\"blue darken-1\"\n                    text\n                    @click=\"dialog_0 = false\"\n                >\n                    Close\n                </v-btn>\n                <v-btn\n                    icon\n                    dialog_0 = \"false\"\n                    @click='changeName'\n                >\n                    <v-icon> mdi-check</v-icon>\n                </v-btn>\n                </v-card-actions>\n            </v-card>\n            </v-dialog>\n            </v-flex>\n        </v-layout>\n    </v-container>\n</template>\n\n<script>\nimport axios from '@/api/index'\nimport { mapState } from 'vuex'\nconst year_start = 1950\nconst year_end = 2030\nconst ageRange = new Array(year_end - year_start + 1).fill(null).map((_, i) => i + year_start)\nconst month_start = 1\nconst month_end = 12\nconst monthRange = new Array(month_end - month_start + 1).fill(null).map((_, i) => i + month_start)\nconst daysRange = [...Array(31)].map((_, i) => i)\n\n\nexport default {\n    data: () => ({\n        year: '',\n        yearrange: ageRange,\n        month: '',\n        monthrange: monthRange,\n        day: '',\n        daysrange: daysRange,\n        dialog_0: false,\n        dialog_1: false,\n        dialog_avatar: false,\n        uploadedImage: '',\n        items: {},\n        avatar : '',\n        lastname: '',\n        firstname: '',\n        railsURL: 'https://54.168.35.214',\n        // railsURL: \"http://localhost:3000\"\n    }),\n\n    async mounted() {\n    await axios()\n      .get('/profile/' + this.user_info[\"id\"],\n      {\n        headers: this.user_info\n      },\n      )\n      .then(response => ( \n            this.avatar = response.data.avatar_url, \n            this.items['Name'] = response.data.name,\n            this.items['Date of birth'] = response.data.birthdate,\n            this.items['E-mail'] = response.data.email\n    )\n    );\n  },\n    methods: {\n        registerBirthdate(){\n            axios()\n            .put('/update_birthdate/',\n            {\n                id: localStorage.getItem('id'),\n                year: this.year,\n                month: this.month,\n                day: this.day\n            },\n            {\n                headers: this.user_info\n            },\n            )\n            .then(response => (\n                console.log(response),\n                // this.$router.go({path: this.$router.currentRoute.path, force: true})\n                this.$router.push({name: 'Profile'})  \n            ))\n        },\n        changeName(){\n            this.name =this.lastname + \" \" +this.firstname\n            axios()\n            .put('/users/' + this.user_info[\"id\"],\n            {\n                name: this.name\n            },\n            {\n                headers: this.user_info\n            },\n            )\n            .then(response => (\n                console.log(response),\n                this.$router.push({name: 'Profile'})  \n            ))\n        },\n        onFileChange(e) {\n            e.preventDefault();\n            let files = e.target.files;\n            this.uploadedImage = files[0];\n            this.profile.avatar = this.uploadedImage\n        },\n        postItem() {\n            let formData = new FormData();\n            formData.append('avatar', this.uploadedImage);\n            formData.append('id', localStorage.getItem('id'));\n            console.log(formData)\n            axios().\n            put('/update_avatar',\n            formData,\n            {\n                headers: this.user_info\n            },\n            ).then(response => {\n            this.uploadedImage = ''\n            this.$refs.file.value = ''\n            this.avatar = response.data.avatar,\n            //   this.$router.go({path: this.$router.currentRoute.path, force: true})\n                this.$router.push({name: 'Profile'})  \n            }).catch(e => {\n            console.log(e)\n            })\n        },\n        returnPage(){\n        this.$router.push({name: 'Profile'})\n        }\n    },\n\n    computed: {\n        ...mapState([\"user_info\"]),\n    }\n}\n</script>\n\n<style>\n  .rounded-card{\n    border-radius:50px;\n}\n.test2{\n\nposition: absolute;\nbottom: 20px;\nmargin: 0px;\npadding: 0px;\nright: 20px;\n}\n</style>"],"sourceRoot":"src/views"}]}