AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAiBA,GA5BA;AA6BA,EAAA,OAAA,EAAA;AACA,IAAA,WADA,yBACA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,MAAA,GACA,KAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA,aAAA;AAAA,QAAA,MAAA,EAAA;AAAA,UAAA,KAAA,EAAA,KAAA,MAAA,CAAA;AAAA;AAAA,OAAA,CADA;AAEA,KAJA;AAKA,IAAA,YALA,wBAKA,MALA,EAKA;AACA,WAAA,MAAA,GAAA,MAAA;AACA;AAPA;AA7BA,CAAA","sourcesContent":["<template>\n \n  <v-container fluid fill-height>\n    <v-card\n      width=\"400px\"\n      height='100%'\n    >\n      <v-list dense>\n        \n        <v-subheader class='text-h5'>送信相手</v-subheader>\n        <v-divider/>\n        <v-list-item-group\n          color=\"primary\"\n          \n        >\n          <v-list-item\n            v-for=\"(item, i) in frineds\"\n            :key=\"i\"\n          >\n              <v-list-item-icon>\n                  <v-avatar size=\"30\">\n                      <img\n                        :src=\"railsURL + item.avatar_url\"\n                      >\n                  </v-avatar>\n                </v-list-item-icon>\n              <v-list-item-content>\n                <v-list-item-title class='text-h8'> {{item.name}}</v-list-item-title>\n              </v-list-item-content> \n              <v-list-item-action>\n                <v-btn @click='selectFrined(item)' icon>\n                  <v-icon>\n                    mdi-arrow-right-bold\n                  </v-icon>\n                </v-btn>\n              </v-list-item-action>\n          </v-list-item>\n          <v-divider/>\n        </v-list-item-group>\n      </v-list>\n    </v-card>\n      <v-card\n          v-if=\"select.name\"\n          class=\"mx-auto\"\n          max-width=\"400\"\n          max-height=\"100%\"\n      >\n        <v-img\n        v-bind:src=\"railsURL + select.avatar_url\"\n        height=\"100%\"\n        width=\"100%\"\n        >\n        \n        </v-img>\n        <v-card-title>\n          {{select.name}}\n        </v-card-title>\n        <v-row>\n            <v-card-subtitle>\n                {{select.birthdate}}\n            </v-card-subtitle>\n        </v-row>\n        <v-container>\n        <v-btn icon x-large @click='editMessage'>\n          <v-icon x-large>\n            mdi-email\n          </v-icon>\n        </v-btn>\n      </v-container>\n      </v-card>\n  </v-container>\n</template>\n\n<script>\nimport axios from '@/api/index'\n\nexport default {\n    data: () => ({\n        frineds: {},\n        dialog: false,\n        select: {\n          'name': null,\n          'avatar': null,\n          'birthdate': null\n        },\n        railsURL: \"http://localhost:3000\",\n    }),\n    async mounted() {\n    await axios()\n      .get('/family/' + localStorage.getItem('id'),\n      {\n        headers: {\n           'access-token': localStorage.getItem('access-token'),\n            uid: localStorage.getItem('uid'),\n            client: localStorage.getItem('client'),\n        },\n      },\n      )\n      .then(response => (\n            console.log(response.data),\n            this.frineds = response.data\n    )\n    );\n    \n  },\n    methods: {\n      editMessage() {\n        console.log(this.select),\n        this.$router.push({name: 'SendMessage', params: {to_id: this.select.id}})\n      },\n      selectFrined(frined) {\n        this.select = frined\n      }\n      \n    },\n}\n</script>"],"sourceRoot":"src/views"}]}