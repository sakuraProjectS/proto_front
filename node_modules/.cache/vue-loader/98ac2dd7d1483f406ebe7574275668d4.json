,\n        ).then(response => {\n          this.uploadedImage = ''\n          this.$refs.file.value = ''\n          this.profile.avatar = response.data.avatar,\n          console.log(response) ,\n        //   this.$router.go({path: this.$router.currentRoute.path, force: true})\n            this.$router.push({name: 'Home'})  \n        }).catch(e => {\n          console.log(e)\n        })\n      },\n        returnPage(){\n        this.$router.push({name: 'Profile'})\n        }\n    },\n}\n</script>\n\n<style>\n  .rounded-card{\n    border-radius:50px;\n}\n.test2{\n\nposition: absolute;\nbottom: 20px;\nmargin: 0px;\npadding: 0px;\nright: 20px;\n}\n</style>"]}]}