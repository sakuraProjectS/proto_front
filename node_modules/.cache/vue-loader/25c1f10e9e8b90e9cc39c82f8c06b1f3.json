 class='mt-0'/>\n            <v-row v-if=\"avatar\" justify=\"center\" class=\"mb-5 mt-5\">\n                <v-avatar size='100'>\n                    <v-img\n                    v-bind:src=\"railsURL + avatar\"\n                    >\n                    </v-img>\n                </v-avatar>\n            </v-row>\n            <v-row v-else justify=\"center\" class=\"mb-5 mt-5\">\n                <v-avatar size='100'>\n                    <v-img\n                    src=\"@/assets/icon/kame.jpg\"\n                    >\n                    </v-img>\n                </v-avatar>\n            </v-row>\n            <v-card\n                height = \"450\"\n                rounded=\"xl\"\n            >\n                <v-container>\n                    <v-row>\n                        <v-col>\n                            <v-list two-line>\n                                <template v-for=\"(item, key, index) in items\">\n                                    <v-list-item :key = key>\n                                        <v-list-item-content>\n                                            <v-list-item-title>\n                                                {{key}}\n                                            </v-list-item-title>\n                                            <v-list-item-subtitle>\n                                                {{item}}\n                                            </v-list-item-subtitle>\n                                        </v-list-item-content>\n                                    </v-list-item>\n                                    <v-divider\n                                        v-if=\"index < Object.keys(items).length\"\n                                        :key=\"item\"\n                                    ></v-divider>\n                                </template>\n                            </v-list>\n                        </v-col>\n                    </v-row>  \n                </v-container>\n            </v-card>\n            </v-flex>\n        </v-layout>\n    </v-container>\n</template>\n\n<script>\nimport axios from '@/api/index'\nimport { mapState } from 'vuex'\n\nexport default {\n    data: () => ({\n      avatar : '',\n      uploadedImage: '',\n      items: {},\n    //   railsURL: 'https://54.168.35.214',\n    //   railsURL: \"http://localhost:3000\"\n    }),\n    async mounted() {\n    await axios()\n      .get('/profile/' + this.user_info[\"id\"],\n      {\n        headers: this.user_info\n      },\n      )\n      .then(response => (\n            this.avatar = response.data.avatar_url, \n            this.items['Name'] = response.data.name,\n            this.items['Date of birth'] = response.data.birthdate,\n            this.items['E-mail'] = response.data.email\n    ));\n  },\n  methods: {\n      editProfile(){\n          this.$router.push({name: 'ProfileEdit'})\n      },\n      returnPage(){\n        this.$router.push({name: 'TimeLine'})\n    }},\n    computed: {\n        ...mapState([\"user_info\", \"railsURL\"]),\n    }\n}\n</script>\n\n<style>\n  .rounded-card{\n    border-radius:50px;\n}\n.test2{\n\nposition: absolute;\nbottom: 20px;\nmargin: 0px;\npadding: 0px;\nright: 20px;\n}\n</style>"]}]}